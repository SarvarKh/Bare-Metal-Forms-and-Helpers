<%= form_tag users_path do %>
   <label>User name:</label>
   <%= text_field_tag :'user[username]' %><br><br>
   
   <label>Email:</label>
   <%= email_field_tag :'user[email]' %><br><br>
  
   <label>Password:</label>
   <%= password_field_tag :'user[password]' %><br><br>

   <%= submit_tag "Create User" %>
<% end %>

